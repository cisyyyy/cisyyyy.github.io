{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Annual Median of Income after Graduation (Age 25â€“34)",
  "data": {
    "url": "/assets/json/Annual%20Median%20of%20Income%20after%20Graduation%20(Age%20of%2025%20-%2034).csv",
    "format": {
      "type": "csv",
      "parse": {
        "Median Income of Associate Degree": "string",
        "Median Income of Bachelor Degree": "string",
        "Median Income of Doctor Degree": "string",
        "Year": "string"
      }
    }
  },
  "transform": [
    {
      "fold": [
        "Median Income of Associate Degree",
        "Median Income of Bachelor Degree",
        "Median Income of Doctor Degree"
      ],
      "as": ["Degree", "IncomeRaw"]
    },
    {
      "calculate": "indexof(datum.IncomeRaw, 'K') > -1 ? toNumber(replace(replace(replace(datum.IncomeRaw,'$',''),',',''),'K',''))*1000 : toNumber(replace(replace(datum.IncomeRaw,'$',''),',',''))",
      "as": "Income"
    }
  ],
  "mark": {"type": "bar", "tooltip": true},
  "encoding": {
    "x": {"field": "Race", "type": "nominal", "axis": {"labelAngle": 0}, "sort": null},
    "y": {"field": "Income", "type": "quantitative", "title": "Annual Median Income", "axis": {"format": "$,.0f"}},
    "color": {
      "field": "Degree",
      "type": "nominal",
      "title": "Degree",
      "scale": {
        "domain": [
          "Median Income of Associate Degree",
          "Median Income of Bachelor Degree",
          "Median Income of Doctor Degree"
        ],
        "range": ["#1f77b4", "#2ca02c", "#ff7f0e"]
      }
    },
    "column": {"field": "Year", "type": "ordinal", "title": "Year", "spacing": 12}
  },
  "config": {
    "view": {"stroke": null},
    "axis": {"labelFontSize": 11, "titleFontSize": 12},
    "legend": {"titleFontSize": 12, "labelFontSize": 11}
  }
}
